<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="413dp"
        android:layout_height="130dp"
        android:contentDescription="TODO"
        android:onClick="voltarescolhatema"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/screen_background_dark"
        tools:ignore="ContentDescription,HardcodedText,VisualLintBounds" />

    <ImageButton
        android:id="@+id/btnironman"
        android:layout_width="197dp"
        android:layout_height="191dp"
        android:layout_marginStart="107dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="107dp"
        android:layout_marginBottom="43dp"
        android:contentDescription="@string/app_name"
        android:onClick="abririronman"
        app:layout_constraintBottom_toTopOf="@+id/btnblackpanther"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@drawable/ironman"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/btnblackpanther"
        android:layout_width="199dp"
        android:layout_height="194dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="105dp"
        android:layout_marginBottom="105dp"
        android:contentDescription="@string/app_name"
        android:onClick="abrirblackpanther"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnironman"
        app:srcCompat="@drawable/pantera" />

    <ImageButton
        android:id="@+id/btnvoltardois"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/app_name"
        android:onClick="voltarescolhatema"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_revert" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="241dp"
        android:layout_height="123dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/marvell"
        tools:ignore="ContentDescription" />

</androidx.constraintlayout.widget.ConstraintLayout>